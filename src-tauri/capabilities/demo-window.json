{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "demo-window",
    "description": "Demo windows: state-sync demo between two windows",
    "windows": ["demo-*"],
    "permissions": [
        "core:default",
        "core:window:default",
        "core:window:allow-show",
        "core:window:allow-hide",
        "core:event:default",
        "core:event:allow-emit",
        "core:event:allow-listen"
    ]
}
